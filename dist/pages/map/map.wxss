@media (-webkit-min-device-pixel-ratio: 1.5), (min-device-pixel-ratio: 1.5) {
  .border-1px::after {
    transform: scaleY(0.7);
  }
}
@media (-webkit-min-device-pixel-ratio: 2), (min-device-pixel-ratio: 2) {
  .border-1px::after {
    transform: scaleY(0.5);
  }
}
.map {
  width: 100%;
  height: 100%;
}
.map .btn {
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: #fff;
  background: #000;
}
.map .my-map {
  width: 100%;
  height: calc(100% - 60rpx);
}
